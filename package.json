{
  "name": "kms-ui",
  "private": true,
  "version": "1.0.0",
  "author": "SAP SE",
  "description": "KMS UI",
  "dependencies": {
    "@openui5/types": "^1.140.0",
    "@types/axios": "^0.14.4",
    "@ui5/cli": "^4.0.26",
    "axios": "^1.12.2",
    "cors": "^2.8.5",
    "fflate": "^0.8.2",
    "http-server": "^14.1.1",
    "js-yaml": "^4.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "ui5-middleware-livereload": "^3.1.3",
    "ui5-tooling-modules": "^3.30.6",
    "ui5-tooling-stringreplace": "^3.4.2",
    "ui5-tooling-transpile": "^3.9.1"
  },
  "scripts": {
    "start": "ui5 serve --port 8081",
    "openchrome": "open -na \"Google Chrome\" \"http://localhost:8081/index.html#/tenant1-id/keyConfigs\" --args --disable-web-security --no-default-browser-check --user-data-dir=\"/tmp/chrome-dev\"",
    "start:test": "ui5 serve --port 8000",
    "build": "npx @ui5/ts-interface-generator && ui5 build --all --include-task=generateCachebusterInfo && npm run strip-maps",
    "strip-maps": "node -e \"const fs=require('fs'); const path=require('path'); const walk=d=>{fs.readdirSync(d).forEach(f=>{const p=path.join(d,f); if(fs.statSync(p).isDirectory()) walk(p); else if(p.endsWith('.js')){const c=fs.readFileSync(p,'utf8').replace(/\\/\\/# sourceMappingURL=.*/g,''); fs.writeFileSync(p,c);}})}; walk('./dist')\"",
    "lint-no-fix": "npx eslint --quiet",
    "check-types": "tsc --noemit",
    "lint": "npx eslint --fix && npm run check-types",
    "serve": "node mock.mjs",
    "start:webserver": "npx http-server dist/ -p 80",
    "test": "npx ui5-test-runner",
    "playwright": "npx playwright test"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@stylistic/eslint-plugin": "^5.3.1",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.18.5",
    "@types/qunit": "^2.19.13",
    "@ui5/linter": "^1.20.0",
    "@ui5/middleware-code-coverage": "^2.0.1",
    "@ui5/ts-interface-generator": "^0.10.3",
    "babel-plugin-istanbul": "^7.0.1",
    "chance": "^1.1.13",
    "dotenv": "^10.0.0",
    "eslint": "^9.35.0",
    "express": "^4.21.2",
    "typescript-eslint": "^8.44.0",
    "ui5-test-runner": "^5.9.1"
  }
}